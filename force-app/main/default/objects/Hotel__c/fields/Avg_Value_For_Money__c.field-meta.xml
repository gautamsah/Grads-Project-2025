<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Avg_Value_For_Money__c</fullName>
    <description>This is the formula to calculate avg rating for value for money</description>
    <externalId>false</externalId>
    <formula>IF( Total_Reviews__c &lt;&gt; 0, Total_Value_For_Money__c  / Total_Reviews__c , 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Avg Value For Money</label>
    <precision>18</precision>
    <required>false</required>
    <scale>1</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
